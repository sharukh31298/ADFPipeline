{
	"name": "Ls_Api_Workspace",
	"properties": {
		"description": "Workspace_Tenant2",
		"parameters": {
			"AppTenantId": {
				"type": "String"
			},
			"AppClientId": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "https://api.powerbi.com/v1.0/myorg/admin/groups?$top=100",
			"enableServerCertificateValidation": true,
			"authenticationType": "OAuth2ClientCredential",
			"clientId": "@{linkedService().AppTenantId}",
			"clientSecret": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyValut",
					"type": "LinkedServiceReference"
				},
				"secretName": "UBTIClientSecretName"
			},
			"tokenEndpoint": "@{concat('https://login.microsoftonline.com/linkedService().AppTenantId/oauth2/v2.0/token')}",
			"scope": "https://analysis.windows.net/powerbi/api/.default"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}