									"sqlWriterUseTableLock": false
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"path": "$['value'][0]['datasourceType']"
											},
											"sink": {
												"name": "principalType",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "$['value'][0]['connectionDetails']['server']"
											},
											"sink": {
												"name": "datasetUserAccessRight",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "$['value'][0]['connectionDetails']['database']"
