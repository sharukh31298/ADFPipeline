{
	"name": "Demo_Workspaces",
	"properties": {
		"activities": [
			{
				"name": "Demo_Workspaces",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "WorkSpaceSourceKey",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['name']"
								},
								"sink": {
									"name": "WorkSpaceName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['kind']"
								},
								"sink": {
									"name": "WorkSpaceType",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['refreshSchedule']['enabled']"
								}
							},
							{
								"source": {
									"path": "['refreshSchedule']['localTimeZoneId']"
								}
							},
							{
								"source": {
									"path": "['refreshSchedule']['notifyOption']"
								}
							},
							{
								"source": {
									"path": "$['@odata.context']"
								}
							},
							{
								"source": {
									"path": "$['@odata.count']"
								}
							},
							{
								"source": {
									"path": "['refreshSchedule']['days'][0]"
								}
							},
							{
								"source": {
									"path": "['refreshSchedule']['times'][0]"
								}
							},
							{
								"source": {
									"path": "['configuredBy'][0]"
								}
							}
						],
						"collectionReference": "$['value']"
					}
				},
				"inputs": [
					{
						"referenceName": "Ls_API_Workspaces",
						"type": "DatasetReference",
						"parameters": {
							"AppTentantId": {
								"value": "@pipeline().parameters.AppTentantId",
								"type": "Expression"
							},
							"AppClientId": {
								"value": "@pipeline().parameters.AppClientId",
								"type": "Expression"
							},
							"AppClientCode": {
								"value": "@pipeline().parameters.AppClientCode",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "LS_SQL_Workspaces",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"AppTentantId": {
				"type": "String"
			},
			"AppClientId": {
				"type": "String"
			},
			"AppClientCode": {
				"type": "String"
			}
		},
		"annotations": []
	}
}